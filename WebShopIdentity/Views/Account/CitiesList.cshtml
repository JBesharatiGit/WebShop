@model IEnumerable<WebShopIdentity.City>

@{
    ViewData["Title"] = "CityList";
}

<h1>CityList</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CityName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CityName)
                </td>
            </tr>
        }
    </tbody>
</table>
